{
  "name": "cloud-101-demo",
  "env": {
    "CLOUD_PROVIDER": {
      "description": "Cloud provider name for demo",
      "value": "Google Cloud Run"
    }
  },
  "run": {
    "image": "gcr.io/$PROJECT_ID/cloud-101-demo",
    "port": 8080,
    "memory": "512Mi",
    "cpu": 1,
    "maxInstances": 3,
    "minInstances": 0,
    "allowUnauthenticated": true
  }
}
